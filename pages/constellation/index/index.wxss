@import "../../../app.wxss";

.bg5 {
    height: auto;
    background-size: cover;
}

.fortune {
    text-align: center;
}

.fortune_box {
    background-size: cover;
}

.b_cloud {
    background: url(data:image/png;base64,) no-repeat 0 0;
    background-size: 100%;
    height: 250rpx;
    position: absolute;
    bottom: -175rpx;
    width: 100%;
}

.fortune .li {
    display: inline-block;
    width: 165rpx;
    height: 200rpx;
    margin: 15rpx 40rpx 0;
    text-align: center;
    font-weight: bold;
}

.fortune .li .li_img {
    height: 150rpx;
    width: 150rpx;
}

.fortune .li .name {
    margin-top: -13rpx;
    font-size: 30rpx;
    color: #000;
}

.fortune .li .date {
    font-size: 26rpx;
    color: #000;
    margin-top: -2px;
}

.mini {
    bottom: -175rpx;
}

@media screen and (min-height:700px) {
    .fortune .li {
        margin: 60rpx 37rpx 0;
    }
}