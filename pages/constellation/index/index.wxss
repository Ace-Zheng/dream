@import "../../../app.wxss";

page {
  background: #FFEAC3;
}
.bg5 {
    height: auto;
    background-size: cover;
}

.fortune {
    text-align: center;
    background: #fff;
    padding: 0 0 30rpx 0;
}

.fortune_box {
    background-size: cover;
    padding: 0 0 50rpx 0;
}

.fortune .li {
    display: inline-block;
    width: 165rpx;
    height: 200rpx;
    margin: 15rpx 10rpx 0;
    text-align: center;
    font-weight: bold;
}

.fortune .li .li_img {
    height: 150rpx;
    width: 150rpx;
}

.fortune .li .name {
    margin-top: -13rpx;
    font-size: 30rpx;
    color: #000;
}

.fortune .li .date {
    font-size: 26rpx;
    color: #000;
    margin-top: -2px;
}

.mini {
    bottom: -175rpx;
}

.fortune_box .xingzuo-compute {
  position: relative;
  width: 710rpx;
  height: 100rpx;
  background: #fff;
  color: #000;
  margin: 20rpx 0 0 0;
  box-shadow:0 0 20px 0 #f71;
  border-radius: 10rpx;
}
.fortune_box .xingzuo-compute .title {
  font-size: 35rpx;
  margin-left: 40rpx;
}
.fortune_box .xingzuo-compute .more {
  width: 45rpx;
  height: 45rpx;
  margin-right: 20rpx;
}
.new {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  top: 0;
  left: 0;
  -webkit-animation: new 5s infinite;
}
@keyframes new{
  0%{
    top: 0;
  }
  5%{
    top: -10rpx;
  }
  10%{
    top: 0;
  }
  60%{}
  80%{}
  100%{}
}

.project {
  width: 100%;
  height: 480rpx;
  background: #fff;
  margin-top: 20rpx;
}
.project > .title {
  width: 100%;
  height: 80rpx;
}
.project > .title .title-name {
  font-size: 32rpx;
  margin: 0 20rpx;
}
.project> .title .line {
  width: 50rpx;
  height: 3rpx;
  background: #000;
}
.project .xingzuo-fenxi {
  box-sizing: border-box;
  position: relative;
  width: 350rpx;
  height: 370rpx;
  background: #F7F7F7;
  border-radius: 5rpx;
  color: #000;
  border-radius: 10rpx;
  padding: 20rpx;
}
.project .xingzuo-fenxi .title {
  margin-bottom: 20rpx;
}
.project .xingzuo-fenxi .img {
  width: 100%;
  height: 200rpx;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.project .xingzuo-fenxi .img image {
  border-radius: 10rpx;
}
.project .xingzuo-fenxi-1 {
  margin-right: 20rpx;
}


.getAuth {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 999;
}

.index_share {
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
  background: #B8E986;
  border-radius: 50%;
  box-shadow: 0 0 30px 0 #fff;
  z-index:1000;
}
.index_share button {
  width: 50rpx;
  height: 50rpx;
  background: #B8E986;
  border-radius: 50%;
  border: 0;
}
.index_share button::after {
  border: 0;
}


@media screen and (min-height:700px) {
    .fortune .li {
        margin: 15rpx 5rpx 0;
    }
}