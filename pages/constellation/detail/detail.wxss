@import"/utils/wxParse/wxParse.wxss";

page {
  background: #FFEAC3;
}

.page-body {
  padding: 0 0 30rpx 0;
}
.detail-body {
    width: 100%;
    min-height: 910rpx;
    font-size: 34rpx;
    margin-top: 40rpx;
    margin-bottom: 60rpx;
    color: #000;
}

.detail-body-title {
    text-align: center;
    font-size: 40rpx;
    font-weight: bold;
}

.detail-body-item {
    padding:0 30rpx;
    line-height:1.5;
    margin-top:40rpx;
    font-size:14px;
}

.fail-body {
    width: 100%;
    min-height: 890rpx;
    text-align: center;
    margin-top: 90rpx;
    margin-bottom: 60rpx;
    line-height: 90rpx;
}

.leo-btn {

}
.leo-btn .search-all {
  width: 20rpx;
  height: 20rpx;
  padding:25rpx;
  margin: 25rpx 40rpx 25rpx 25rpx;
}
.leo-btn .look-all {
  width: 20rpx;
  height: 20rpx;
  padding:25rpx;
  margin:25rpx;
}

.index_share {
  position: fixed;
  right: 20rpx;
  bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
  background: #B8E986;
  border-radius: 50%;
  box-shadow: 0 0 30px 0 #fff;
  z-index:1000;
}
.index_share button {
  width: 50rpx;
  height: 50rpx;
  background: #B8E986;
  border-radius: 50%;
  border: 0;
}
.index_share button::after {
  border: 0;
}