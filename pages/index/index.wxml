          <image src="https://tpdoc.cn/uploads/dream/guishen.png"></image>

          <button wx:if="{{!mark2}}" bindtap="itemLockClick" id="2" class="cover abc-flex-x-center" openType="share">
            <image class="lock" src="/images/lock.png"></image>
            <image class="text" src="/images/share-text.png"></image>
          </button>
        </navigator>

        <view class="guishen-par abc-flex-y-between">
