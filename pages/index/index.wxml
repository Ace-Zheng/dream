
<button wx:if="{{!userInfo || !userInfo.nickName}}" class="getAuth" open-type="getUserInfo" bindgetuserinfo="bindGetUserInfo"></button>
<view class="page">
    <view class="title abc-flex-x-center">
      <view><view class="line"></view></view>
      <view class="title-name">分类</view>
      <view><view class="line"></view></view>
    </view>

    <view class="content">
      <view class="type-item abc-flex-x-center">
        <navigator bindtap="itemClick" id="0" url="/pages/titlelist/titlelist?index=0&title={{groupData[0]}}"><image src="https://tpdoc.cn/uploads/dream/ziran.png"></image></navigator>
        <navigator bindtap="itemClick" id="1" url="/pages/titlelist/titlelist?index=0&title={{groupData[1]}}"><image src="https://tpdoc.cn/uploads/dream/jianzhu.png"></image></navigator>
      </view>

      <view class="type-item second abc-flex-x-center">
